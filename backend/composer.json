{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/doctrine-orm": "^4.2.9",
        "api-platform/symfony": "^4.2.9",
        "ardagnsrn/ollama-php": "^1.0.5",
        "codewithkyrian/whisper.php": "^1.1",
        "doctrine/dbal": "^3.10.4",
        "doctrine/doctrine-bundle": "^2.18.1",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.5.8",
        "erusev/parsedown": "^1.7.4",
        "gemini-api-php/client": "^1.7.2",
        "google/apiclient": "^2.18.4",
        "google/protobuf": "^4.33.2",
        "google/recaptcha": "^1.3.1",
        "grpc/grpc": "^1.74",
        "jumbojett/openid-connect-php": "^1.0.2",
        "league/flysystem-aws-s3-v3": "^3.30.1",
        "lucianotonet/groq-php": "^1.3",
        "nelmio/api-doc-bundle": ">=5.8.2",
        "nelmio/cors-bundle": "^2.6",
        "netflie/whatsapp-cloud-api": "^2.2.1",
        "openai-php/client": "^0.17.1",
        "phpdocumentor/reflection-docblock": "^5.6.5",
        "phpoffice/phppresentation": "^1.2",
        "phpoffice/phpword": "^1.4",
        "phpstan/phpdoc-parser": "^2.3",
        "smalot/pdfparser": "^2.12.2",
        "stripe/stripe-php": "^16.6",
        "symfony/asset": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/doctrine-messenger": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/flex": "^2.10",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/lock": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/messenger": "7.3.*",
        "symfony/monolog-bundle": "^4.0",
        "symfony/process": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/rate-limiter": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/validator": "7.3.*",
        "symfony/yaml": "7.3.*",
        "web-token/jwt-library": "^4.1",
        "web-token/jwt-signature": "^2.2",
        "web-token/jwt-signature-algorithm-ecdsa": "^2.2",
        "web-token/jwt-signature-algorithm-rsa": "^2.1",
        "webklex/php-imap": "^6.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "codewithkyrian/platform-package-installer": true
        },
        "audit": {
            "ignore": ["PKSA-365x-2zjk-pt47", "PKSA-64jn-3d9t-gncx"]
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Inference\\": "lib/grpc/Inference/",
            "GPBMetadata\\": "lib/grpc/GPBMetadata/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@proto:generate",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@proto:generate",
            "@auto-scripts"
        ],
        "lint": "php-cs-fixer check --diff",
        "lint:fix": "php-cs-fixer fix",
        "phpstan": "phpstan analyse",
        "proto:generate": [
            "protoc --php_out=lib/grpc --proto_path=proto/triton proto/triton/grpc_service.proto proto/triton/model_config.proto"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.3.1",
        "friendsofphp/php-cs-fixer": "^3.91.3",
        "phpstan/phpstan": "^2.1.33",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpunit/phpunit": "^12.5.2",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/maker-bundle": "^1.65.1"
    }
}
