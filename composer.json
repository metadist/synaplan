{
    "require": {
        "league/flysystem": "^3.0",
        "netflie/whatsapp-cloud-api": "^2.2",
        "phpmailer/phpmailer": "^6.9",
        "openai-php/client": "^0.13.0",
        "codewithkyrian/whisper.php": "^1.1",
        "ardagnsrn/ollama-php": "^1.0",
        "smalot/pdfparser": "^2.11",
        "phpoffice/phpword": "^1.3",
        "phpoffice/phppresentation": "^1.1",
        "lucianotonet/groq-php": "^0.0.10",
        "google/apiclient": "^2.18.0",
        "gemini-api-php/client": "^1.7",
        "erusev/parsedown": "^1.7",
        "webklex/php-imap": "^6.2",
        "stevenmaguire/oauth2-microsoft": "^2.2",
        "jumbojett/openid-connect-php": "^1.0",
        "grpc/grpc": "^1.42",
        "google/protobuf": "^3.21",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Inference\\": "app/inc/ai/grpc-triton/Inference/",
            "GPBMetadata\\": "app/inc/ai/grpc-triton/GPBMetadata/"
        }
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup",
        "update-protos": [
            "curl -L https://raw.githubusercontent.com/triton-inference-server/common/main/protobuf/grpc_service.proto -o ./dev/triton/grpc_service.proto",
            "curl -L https://raw.githubusercontent.com/triton-inference-server/common/main/protobuf/model_config.proto -o ./dev/triton/model_config.proto"
        ],
        "generate-proto": [
            "protoc --php_out=app/inc/ai/grpc-triton --proto_path=./dev/triton ./dev/triton/grpc_service.proto ./dev/triton/model_config.proto"
        ],
        "post-install-cmd": ["@generate-proto"],
        "post-update-cmd": ["@generate-proto"]

    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "codewithkyrian/platform-package-installer": true
        }
    }
}
